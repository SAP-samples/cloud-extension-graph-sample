{
  "name": "georel-graph",
  "version": "2.0.0",
  "description": "Application for Customer GeoRelation Processing",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^6",
    "@sap/xb-msg-amqp-v100": "^0.9.53",
    "express": "^4",
    "hdb": "^0.18.3",
    "@sap/xssec": "^3.2.2",
    "@sap/audit-logging": "^5.0.0",
    "passport": "^0.4.1",
    "@sap-cloud-sdk/http-client": "^2.5.0",
    "@sap-cloud-sdk/util": "^2.5.0"
  },
  "devDependencies": {
    "sqlite3": "^5.0.2",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "mocha": "^9.0.2"
  },
  "scripts": {
    "start": "cds run",
    "debug": "cds run --with-mocks --in-memory",
    "watch-geo-customer": "cds watch --open geo-customer/webapp/index.html#fe-lrop-v4",
    "test:integration": "mocha tests/integration/test.js --timeout 15000 --exit"
  },
  "cds": {
    "features": {
      "fetch_csrf": true
    },
    "log": {
      "levels": {
        "remote": "debug"
      }
    },
    "requires": {
      "messaging": {
        "kind": "local-messaging",
        "[production]": {
          "kind": "enterprise-messaging-shared",
          "queue": {
            "name": "sap/geograph/1/georel-graph-srv/1234"
          },
          "amqp": {
            "incomingSessionWindow": 30
          },
          "format": "cloudevents"
        },
        "[hybrid]": {
          "kind": "enterprise-messaging-shared",
          "queue": {
            "name": "sap/geograph/1/georel-graph-srv/1234"
          },
          "amqp": {
            "incomingSessionWindow": 30
          },
          "format": "cloudevents"
        }
      },
      "db": {
        "kind": "sql",
        "--credentials": {
          "database": "geo.db"
        }
      },
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "kind": "dummy-auth"
      },
      "[production]": {
        "auth": {
          "kind": "jwt-auth"
        }
      },
      "GRAPH_CC": {
        "kind": "odata",
        "credentials": {
          "path": "/v1/sap.s4",
          "destination": "graph-api-cc"
        }
      },
      "sap.s4": {
        "kind": "odata",
        "model": "srv/external/sap.s4",
        "credentials": {
          "path": "/v1/sap.s4",
          "destination": "graph-api-cc"
        }
      },
      "sap.graph": {
        "kind": "odata",
        "model": "srv/external/sap.graph",
        "credentials": {
          "path": "/v1/sap.graph",
          "destination": "graph-api-cc"
        }
      },
      "sap.c4c": {
        "kind": "odata",
        "model": "srv/external/sap.c4c",
        "credentials": {
          "path": "/v1/sap.c4c",
          "destination": "graph-api-cc"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "sapux": [
    "app/geo-customer"
  ]
}
